{
  "name": "pear-doctor",
  "version": "0.1.0",
  "description": "> The Pear Doctor Application",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "format": "prettier --write .",
    "lint": "prettier --check .",
    "test": "prettier --check ."
  },
  "pear": {
    "routes": ".",
    "unrouted": [
      "/subprocess/index.js"
    ],
    "pre": "pear-electron/pre",
    "name": "pear-doctor",
    "gui": {
      "backgroundColor": "#151517",
      "height": 1400,
      "width": 1100,
      "closeHides": true,
      "userAgent": "Slackbot-LinkExpanding 1.0 (+https://api.slack.com/robots)"
    },
    "links": [
      "http://127.0.0.1"
    ]
  },
  "dependencies": {
    "hypercore-crypto": "^3.6.1",
    "hyperdb": "^4.15.0",
    "hyperschema": "^1.12.4",
    "pear-bridge": "^1.2.4",
    "pear-electron": "^1.7.22",
    "pear-run": "^1.0.1",
    "pear-user-dirs": "^1.0.2",
    "redhat-overpass-font": "^1.0.0",
    "which-runtime": "^1.3.0",
    "z32": "^1.1.0"
  },
  "devDependencies": {
    "brittle": "^3.4.0",
    "pear-interface": "^1.0.3",
    "prettier": "^3.6.2",
    "prettier-config-holepunch": "^1.0.0"
  },
  "directories": {
    "lib": "lib"
